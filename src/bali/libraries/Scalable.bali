            $type: /bali/libraries/Scalable/v1
            $parameters: [
                $first: [
                    $description: "
                        This parameter specifies the scalable component to be added to.
                    "($mediaType: "application/bsmd")
                    $type: /bali/libraries/Scalable/v1
                ]($type: /bali/aspects/Parameter/v1)
                $second: [
