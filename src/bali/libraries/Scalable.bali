            ]($type: /bali/collections/Catalog/v1($valueType: /bali/aspects/Parameter/v1))
        ]($type: /bali/aspects/Function/v1)
        $Difference: [
            $description: "
                This function returns the arithmetic difference of the specified scalable components.
            "($mediaType: "application/bsmd")
            $type: /bali/libraries/Scalable/v1
            $parameters: [
                $first: [
                    $description: "
                        This parameter specifies the scalable component to be subtracted from.
                    "($mediaType: "application/bsmd")
                    $type: /bali/libraries/Scalable/v1
                ]($type: /bali/aspects/Parameter/v1)
                $second: [
                    $description: "
                        This parameter specifies the scalable component to be subtracted from the first component.
